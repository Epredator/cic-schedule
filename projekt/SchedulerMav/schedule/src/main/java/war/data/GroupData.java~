	public static IndexedContainer getInvStatus() {
	    IndexedContainer c = new IndexedContainer();
	    fillGroupData(c);
	    return c;
	}
	
	public static void fillGroupData(IndexedContainer container) {
		container.addContainerProperty(invStatus, String.class,
	            null);
